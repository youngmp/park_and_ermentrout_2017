nu1'=1+g(nu1,lambda)*((-(1.04609/(1+nu1^2))-0.0698954/(1+4*nu1^2)-5.00247*10^(-6)/(1+9* nu1^2)+(1.04609* (1+lambda))/(nu1^2+(1+lambda)^2)+(0.0698954* (1+lambda))/(4 *nu1^2+(1+lambda)^2)+(5.00247*10^(-6)* (1+lambda))/(9* nu1^2+(1+lambda)^2))/lambda)

nu2'=1+g(nu2,lambda)*((-0.647456 - 0.463104/(1 + nu2^2) - 0.0054351/(1 + 4*nu2^2) + 0.647456/( 1. + lambda) + (0.463104* (1 + lambda))/( nu2^2 + (1 + lambda)^2) + (0.0054351* (1 + lambda))/( 4* nu2^2 + (1 + lambda)^2))/lambda)

g(nu,lambda)=nu/(nu*(1.04609/(1 + nu^2) + 0.0698954/(1 + 4*nu^2) + 5.00247*10^(-6)/( 1 + 9*nu^2)))

p lambda=1